module.exports.retry = function (wait, cb, times, time) {
  //@param function wait 等待的条件
  //@param function cb 条件满足后做的操作
  //@param number times  尝试的次数
  //@param number time  每次等待的时间
  var _times = 0;

  function _try() {
    if (_times >= times) return;
    if (wait()) cb();else {
      _times++;
      setTimeout(_try, time);
    }
  }
  _try();
};
