    var message = this._resolveLottery(voteStr, reply.content);\n\n      h = '<p class=\"text\">' + (reply.isTop || reply.isUpTop ? '<span class=\"stick\"></span>' : '') + message + '</p>';\n    }\n\n    return h;\n  };\n\n  Feedback.prototype._createVoteContent = function (content) {\n    if (content.vote && content.vote.id && !content.lottery) {\n      content.vote.title = utils.unhtml(content.vote.title);\n      var dom = ['<div class=\"vote-container\" data-id=\"' + content.vote.id + '\" data-name=\"' + content.vote.title + '\">', '<div class=\"image-area\">', '<div class=\"share-img\"></div>', '</div>', '<div class=\"text-area\">', '<div class=\"text-content\">' + content.vote.title + '</div>', '<div class=\"text-desc\">' + content.vote.cnt + '</div>', '<div class=\"vote-btn-box\">', '<div class=\"vote-btn\"></div>', '</div>', '</div>', '</div>'].join('');\n      return dom;\n    } else {\n      return '';\n    }\n  };\n\n  Feedback.prototype._createLotteryContent = function (content) {\n    if (content.lottery && content.lottery.lottery_id) {\n      content.lottery.title = utils.unhtml(content.lottery.title);\n      var now = new Date().getTime();\n      var desc;\n\n      if (content.lottery.lottery_status === 2) {\n        desc = '';\n      } else {\n        desc = utils.formatLotteryTime(content.lottery.lottery_time);\n      }\n\n      var dom = ['<div class=\"lottery-container\" data-id=\"' + content.lottery.lottery_id + '\">', '<div class=\"image-area\">', '<div class=\"share-img\"></div>', '</div>', '<div class=\"text-area\">', '<div class=\"text-content\"></div>', '<div class=\"text-desc\">' + desc + '</div>', content.lottery.lottery_status === 2 ? '<div class=\"lottery-btn-box\"><div class=\"lottery-btn\"></div></div>' : '', '</div>', '</div>'].join('');\n      return dom;\n    } else {\n      return '';\n    }\n  };\n  /**\n   * up\n   * @return {boolean}\n   * @private\n   */\n\n\n  Feedback.prototype._canSetTop = function (reply) {\n    if (!this.us