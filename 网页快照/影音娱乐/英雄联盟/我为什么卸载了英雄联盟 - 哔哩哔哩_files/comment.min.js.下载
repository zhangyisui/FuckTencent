);\n    }\n\n    if (/cv/i.test(str)) {\n      return '//www.bilibili.com/read/' + str;\n    }\n  }; // url\n\n\n  Feedback.prototype._resolveUrl = function (str) {\n    // https://regex101.com/r/PtKCX3/2\n    // var reg = /(http(s)?:\\/\\/)?([0-9a-zA-Z]+.)?((b23\\.tv)|(kaola\\.com)|(sugs\\.suning\\.com)|(biliapi\\.net)|((bilibiliyoo|im9)\\.com)|(biligame\\.(com|cn))|(bilibili\\.(com|tv|cn)))(\\/?|<![^@])([-\\.\\_0-9A-Za-z\\/]+(\\/)?)?((\\?|\\&)(\\w+=\\w+))*(\\#[0-9a-zA-Z]+)?(?![^<>]*>|[^\"]*?<\\/a)/ig\n    var reg = /(?:(?:https?):\\/\\/)?(?:(?:[a-z0-9](?:[a-z0-9_\\-]{0,61}[a-z0-9]){0,1}\\.)*(?:b23\\.tv|kaola\\.com|sugs\\.suning\\.com|biliapi\\.net|(?:bilibiliyoo|im9)\\.com|biligame\\.c(?:om|n)|bilibili\\.(?:tv|c(?:om|n))))(?:[/\\?](?:(?:[a-z0-9;/\\?:@&=#~\\-\\.\\+!\\*'\\(\\),_\\$])|(?:%[a-f0-9]{2}))*)?(?![^<>]*>|[^\"]*?<\\/a)/gi; // var match = str.match(reg)\n\n    return str.replace(reg, function (match) {\n      var unhtmlMatch = unhtml(match);\n      var url = unhtmlMatch;\n\n      try {\n        match = decodeURI(s);\n      } catch (e) {}\n\n      if (match.indexOf('http') !== 0) {\n        url = '//' + url;\n      }\n\n      return '<a href=\"' + url + '\" target=\"_blank\">' + unhtmlMatch + '</a>';\n    });\n  };\n  /**\n   * \n   */\n\n\n  Feedback.prototype._resolveSeek = function (str) {\n    var me = this;\n\n    if (this.pageType === 1) {\n      var seekReg = /(\\d+\\#)?(\\d+(\\:|\\)){1,2}\\d\\d/g;\n      var matchResult = str.match(seekReg);\n\n      if (matchResult) {\n        matchResult.forEach(function (item) {\n          if (item.indexOf('#') > 0) {\n            var p = parseInt(item.split('#')[0]);\n            var timeStr = item.split('#')[1];\n          } else {\n            var p = -1;\n            var timeStr = item;\n          }\n\n          if (me._timeStrToNumber(timeStr)) {\n            var time = me._timeStrToNumber(timeStr);\n\n            str = str.replace(item, '<a class=\"video-seek\" data-p=\"' + p + '\" data-time=\"' + time + '\">' + item + '</a>');\n          }\n        });\n      }\n    }\n\n    return str;\n  };\n  /**\n   * \n   */\n\n\n  Feedback.prototype._resolveVote = function (str, content) {\n    if (content.vote) {\n      return str.replace(/\\{vote:.*}/, '<a class=\"vote-link\" data-id=\"' + content.vote.id + '\" data-name=\"' + utils.unhtml(content.vote.title) + '\"><img src=\"data:image/svg+xml;base64,\" alt=\"\" />' + utils.unhtml(content.vote.title) + '</a>');\n    } else {\n      return str;\n    }\n  };\n  /**\n   * \n   */\n\n\n  Feedback.prototype._resolveLottery = function (str, content) {\n    if (content.lottery) {\n      return str.replace(/\\{lottery:.*}/, '<a class=\"lottery-link\" data-id=\"' + content.lottery.lottery_id + '\"><img src=\"data:image/png;base64,\" alt=\"\" /></a>');\n    } else {\n      return str;\n    }\n  };\n  /**\n   * @av\n   * @param {String} str - \n   * @param {String[]} map - @map\n   * @private\n   */\n\n\n  Feedback.prototype._resolveAt = function (str, map) {\n    return str.replace(/(?!<a[^>]*>)@[^\\s]+(?![^<]*<\\/a>)/g, function (match) {\n      return map[match.trim().substring(1)] || match;\n    }).replace(/(?!<a[^>]*>)(bv1)(\\w{9})(?![^<]*<\\/a>)/ig, this.showBv ? '<a href=\"//www.bilibili.com/video/bv1$2/\" target=\"_blank\">$1$2</a>' : '$1$2').replace(/(?!<a[^>]*>)av([0-9]+)(?![^<]*<\\/a>)/ig, '<a href=\"//www.bilibili.com/video/av$1/\" target=\"_blank\" data-view=\"$1\">av$1</a>').replace(/(?!<a[^>]*>)cv([0-9]+)(?![^<]*<\\/a>)/ig, '<a href=\"//www.bilibili.com/read/cv$1/\" target=\"_blank\" data-view=\"$1\">cv$1</a>').replace(/\\n/g, \"<br />\");\n  };\n  /**\n   * \n   */\n\n\n  Feedback.prototype._timeStrToNumber = function (timeStr) {\n    var arr = timeStr.split(/\\:|\\/);\n\n    if (arr.length === 3 && arr[1].length === 2 && arr[2].length === 2) {\n      var hour = parseInt(arr[0]);\n      var minute = parseInt(arr[1]);\n      var second = parseInt(arr[2]);\n\n      if (minute >= 0 && minute < 60 && second >= 0 && second < 60) {\n        return second + minute * 60 + hour * 3600;\n      }\n    }\n\n    if (arr.length === 2 && arr[1].length === 2) {\n      var minute = parseInt(arr[0]);\n      var second = parseInt(arr[1]);\n\n      if (second >= 0 && second < 60) {\n        return second + minute * 60;\n      }\n    }\n  };\n\n  Feedback.prototype._formateMemberMap = function (members) {\n    var obj = {};\n    var item;\n\n    for (var i = 0, len = members.length; i < len; ++i) {\n      item = members[i];\n      obj[item.uname] = '<a href=\"//space.bilibili.com/' + item.mid + '\" target=\"_blank\" data-usercard-mid=\"' + item.mid + '\">@' + utils.unhtml(item.uname) + ' </a>';\n    }\n\n    return obj;\n  };\n\n  Feedback.prototype._createReplyBox = function (options) {\n    var _this = this;\n\n    options = options || {};\n    var cla = '';\n    var disableText = '',\n        subDisableText = '';\n    var placeHolder = this.control.root_input_text || '';\n\n    if (!this.userStatus.isLogin) {\n      disableText = '<a class=\"b-btn btn-open-mini-Login\"></a> ()';\n      subDisableText = '<a class=\"b-btn btn-open-mini-Login\"></a> ()';\n      cla = 'no-login';\n    } else if (options.forbid || this.control.input_disable) {\n      disableText = this.control.root_input_text;\n      subDisableText = this.control.child_input_text;\n      cla = 'no-login';\n    } else if (this.disableText) {\n      disableText = this.disableText;\n      cla = 'no-login';\n    }\n\n    var renderFace = function renderFace(userStatus) {\n      var _userStatus$officialV;\n\n      var iconType = {\n        0: 'personal',\n        1: 'business'\n      }[userStatus === null || userStatus === void 0 ? void 0 : (_userStatus$officialV = userStatus.officialVerify) === null || _userStatus$officialV === void 0 ? void 0 : _userStatus$officialV.type];\n      var userAvatar = new UserAvatar[\"default\"]({});\n      var pendant = userStatus.pendant;\n      var pendantImg = utils.isWebp ? pendant === null || pendant === void 0 ? void 0 : pendant.image_enhance : (pendant === null || pendant === void 0 ? void 0 : pendant.image_enhance_frame) || (pendant === null || pendant === void 0 ? void 0 : pendant.image);\n      var avatarStr = userAvatar.append({\n        avatar: userStatus.face,\n        pendent: _this.appMode !== 'comic' ? pendantImg : '',\n        avatarWidth: 65,\n        renderWidth: 48,\n        isNFT: userStatus === null || userStatus === void 0 ? void 0 : userStatus.face_nft_new,\n        isCenterScale: true,\n        iconType: iconType,\n        isAnimate: !utils.isWebp && Boolean(pendant === null || pendant === void 0 ? void 0 : pendant.image_enhance_frame)\n      }); //\n\n      var face = ['<div class=\"user-face\">', avatarStr, '</a>', '</div>'].join('');\n      return face;\n    };\n\n    var dynamicRepostDom = options.forbid ? '' : ['<div class=\"dynamic-repost\">', '<label>', '<input type=\"checkbox\" class=\"dynamic-repost-checkbox\" />', '<span></span>', '</label>', '</div>'].join('');\n    var emojiBtnDom = options.forbid ? '' : ['<div class=\"comment-emoji\">', '<i class=\"face\"></i>', '<span class=\"text\"></span>', '</div>'].join('');\n    return ['<div class=\"comment-send ' + cla + '\">', renderFace(this.userStatus), '<div class=\"textarea-container\">', disableText ? '<div class=\"baffle-wrap\"><div class=\"baffle\">' + (options.isSub ? subDisableText : disableText) + '</div></div>' : '', '<i class=\"ipt-arrow\"></i>', '<textarea cols=\"80\" name=\"msg\" rows=\"5\" placeholder=\"' + placeHolder + '\" class=\"ipt-txt\"></textarea>', '<button type=\"submit\" class=\"comment-submit\" ' + (cla ? 'disabled=\"disabled\"' : '') + '></button>', '</div>', emojiBtnDom, !options.isSub && this.dynamicRepost ? dynamicRepostDom : '', '</div>'].join('');\n  };\n\n  Feedback.prototype._createLiteReplyBox = function (options) {\n    options = options || {};\n    var cla = '';\n    var disableText = '',\n        subDisableText = '';\n    var placeHolder = this.control.root_input_text || '';\n\n    if (!this.userStatus.isLogin) {\n      disableText = '<a class=\"b-btn btn-open-mini-Login\"></a> ()';\n      subDisableText = '<a class=\"b-btn btn-open-mini-Login\"></a> ()';\n      cla = 'no-login';\n    } else if (options.forbid || this.control.input_disable) {\n      disableText = this.control.root_input_text;\n      subDisableText = this.control.child_input_text;\n      cla = 'no-login';\n    } else if (this.disableText) {\n      disableText = this.disableText;\n      cla = 'no-login';\n    }\n\n    var emojiBtnDom = options.forbid ? '' : ['<div class=\"comment-emoji comment-emoji-lite\">', '<i class=\"face\"></i>', '<span class=\"text\"></span>', '</div>'].join('');\n    return ['<div class=\"comment-send-lite ' + cla + '\">', '<div class=\"textarea-container\">', disableText ? '<div class=\"baffle-wrap\"><div class=\"baffle\">' + (options.isSub ? subDisableText : disableText) + '</div></div>' : '', '<textarea class=\"ipt-txt\" placeholder=\"' + placeHolder + '\"/></textarea>', '<button type=\"submit\" class=\"comment-submit\" ' + (cla ? 'disabled=\"disabled\"' : '') + '></button>', '</div>', emojiBtnDom, '</div>'].join('');\n  };\n  /**\n   * \n   * @param {Number} p - \n   * @param {Boolean} scroll - \n   */\n\n\n  Feedback.prototype.loadPage = function (p, scroll) {\n    var me = this;\n    me.loading = true;\n\n    if (scroll) {\n      $(window).scrollTop(me.$root.offset().top);\n    }\n\n    if (this.abtest.optimize) {\n      if (this.isEnd) {\n        return;\n      }\n\n      this.$root.find('.loading-state').html('...');\n    }\n\n    var loadFunction = this.abtest.optimize || this.noPage ? this._getReplyByCursor() : this._getReplyByPage(p); // $.when(this._getLocs([me.notice.locsId]), loadFunction).done(function(v1, v2) {\n\n    loadFunction.done(function (res2) {\n      me._hideLoading();\n\n      me.loaded = true;\n\n      if (res2 && res2.code === 0) {\n        if (res2.data) {\n          var cm_info = res2.data.cm_info;\n\n          if (cm_info && cm_info.ads && Object.keys(cm_info.ads).length > 0) {\n            me.notice.locsId = Object.keys(cm_info.ads)[0];\n            me.notice.data = cm_info.ads[me.notice.locsId][0];\n            me.notice.type = 'cm';\n            me.notice.isAd = Boolean(cm_info.ads[me.notice.locsId][0].is_ad);\n          }\n\n          if (me.notice.isAd === false && res2.data.notice) {\n            me.notice.data = res2.data.notice;\n            me.notice.type = 'notice';\n          }\n\n          me.mode = res2.data.mode || res2.data.cursor.mode;\n          me.show_type = res2.data.cursor ? res2.data.cursor.show_type : res2.data.control ? res2.data.control.show_type : 1, me.blacklist = res2.data.blacklist || 0;\n          me.assist = res2.data.assist || 0;\n          me.pageInfo = res2.data.page || {};\n          me.readOnly = res2.data.config.read_only;\n          me.folder = res2.data.folder || {};\n          me.showBv = res2.data.show_bvid || false;\n          var isLastPage = me.pageInfo.num * me.pageInfo.size >= me.pageInfo.count;\n          var control = res2.data.control;\n\n          if (control) {\n            me.control.bg_text = control.bg_text;\n            me.control.child_input_text = control.child_input_text;\n            me.control.input_disable = control.input_disable;\n            me.control.root_input_text = control.root_input_text;\n            me.control.web_selection = control.web_selection;\n          }\n\n          me._renderTabs(res2);\n\n          me._renderNotice(me.notice);\n\n          me._updateReplyListByData(res2.data, p, isLastPage);\n\n          if (me.abtest.optimize) {\n            me.next = res2.data.cursor.next;\n            me.isEnd = res2.data.cursor.is_end;\n\n            if (me.isEnd) {\n              me.$root.find('.loading-state').html('');\n            } else {\n              me.$root.find('.loading-state').html('');\n            }\n          } else {\n            me._renderBottomPagination(me.pageInfo);\n          }\n\n          me._fixEmojiPosition();\n\n          if (res2.data.page) {\n            // \n            $('.common .b-head .results').html(res2.data.page.acount || 0).show();\n          } else if (res2.data.cursor) {\n            $('.common .b-head .results').html(res2.data.cursor.all_count || 0).show();\n          }\n\n          $('.common .b-head .del-log').remove(); //\n\n          if (!me.$root.find('.bb-comment > .comment-send')[0]) {\n            $(me._createReplyBox()).insertBefore(me.$root.find('.comment-list'));\n          }\n\n          if (me.abtest.optimize && me.$root.find('.comment-send-lite').length === 0) {\n            $(me._createLiteReplyBox()).insertAfter(me.$root.find('.comment-list'));\n            me.$root.find('.comment-send-lite').css('width', me.$root.width() + 13);\n          }\n\n          me.trigger('pageLoaded', isLastPage, res2.data); //\n\n          if (me.jumpToParent) {\n            scrollTo(0, me.$root.offset().top);\n          }\n        }\n      } else {\n        me.$root.find('.bb-comment > .comment-send').remove();\n        me.control = {\n          bg_text: \"\",\n          child_input_text: res2.message || '',\n          input_disable: false,\n          root_input_text: res2.message || '',\n          web_selection: false\n        };\n        me.isEnd = true;\n        me.$root.find('.loading-state').html('');\n        $(me._createReplyBox({\n          forbid: true\n        })).insertBefore(me.$root.find('.comment-list'));\n        me.$root.find('.comment-header .tabs-order .hot-sort').html('');\n        me.$root.find('.comment-header .tabs-order .new-sort').hide();\n\n        me._renderReplyList([]);\n      }\n    }).fail(function () {\n      if (!me.loaded) {\n        me._showLoading('<span><a class=\"reload-comment\"></a></span>');\n      }\n    }).always(function () {\n      me.loading = false;\n    });\n  };\n\n  Feedback.prototype._renderTabs = function (res) {\n    if (this.firstLoad) {\n      return;\n    }\n\n    this.firstLoad = true;\n    utils.customReport('replyShow', {\n      ordering: this.mode === 3 ? 'heat' : 'time',\n      type: this.pageType,\n      oid: this.oid\n    });\n    var defaultTab = this.$root.find('.comment-header .tabs-order .default-sort');\n    var newTab = this.$root.find('.comment-header .tabs-order .new-sort');\n    var hotTab = this.$root.find('.comment-header .tabs-order .hot-sort');\n\n    if (this.control.web_selection) {\n      hotTab.html('');\n      defaultTab.hide();\n      newTab.hide();\n      return;\n    }\n\n    if (this.mode === 2 || this.mode === 3) {\n      hotTab.html('');\n      defaultTab.hide().removeClass('on'); //  1: /\n\n      if (this.show_type === 1) {\n        newTab.show();\n        hotTab.show();\n      } //  2:PC hotTab\n      else if (this.show_type === 2) {\n        hotTab.show();\n        newTab.hide();\n      } //  2-\n\n\n      if (this.mode === 2) {\n        newTab.addClass('on');\n        hotTab.removeClass('on');\n        this.sort = 0;\n\n        if (!this.jumpId) {\n          this.loadPage(1);\n        }\n      } //  3-\n      else if (this.mode === 3) {\n        newTab.removeClass('on');\n        hotTab.addClass('on');\n      }\n    }\n\n    if (res.data.page && res.data.page.count >= 20 || res.data.hots || this.sort === 2) {\n      hotTab.show();\n    } else {\n      hotTab.hide();\n    }\n\n    if (res.data.hots) {\n      this.isViewHot = true;\n    } else {\n      this.isViewHot = false;\n    }\n  };\n\n  Feedback.prototype._updateReplyListByData = function (data, p, isLastPage) {\n    var topReplies = [];\n    var pos = -1;\n    var me = this; // replyreply/jumpres.data.upper,\n    // reply/mainres.data.top\n    // ,data.upper\n\n    for (var i in data.top) {\n      data.upper[i] = data.top[i];\n    } // upid\n\n\n    me.upId = data.upper && data.upper.mid;\n    me.hasUpTop = data.upper && data.upper.top;\n\n    if (data.lottery_card && !me.next) {\n      data.lottery_card.isUpTop = true;\n      topReplies.push(data.lottery_card);\n    } // \n    // replyreply/jumpreply/main\n\n\n    if (this.appMode === 'comic' && data.top && (me.pageInfo.num === 1 || !me.next)) {\n      data.top.isTop = true;\n      topReplies.push(data.top);\n    }\n\n    if (data.upper && data.upper.admin && (me.pageInfo.num === 1 || !me.next)) {\n      data.upper.admin.isTop = true;\n      topReplies.push(data.upper.admin);\n    }\n\n    if (data.upper && data.upper.top && (me.pageInfo.num === 1 || !me.next)) {\n      data.upper.top.isUpTop = true;\n      topReplies.push(data.upper.top);\n    }\n\n    if (data.upper && data.upper.upper && (me.pageInfo.num === 1 || !me.next)) {\n      data.upper.upper.isUpTop = true;\n      topReplies.push(data.upper.upper);\n    }\n\n    if (data.upper && data.upper.vote && (me.pageInfo.num === 1 || !me.next)) {\n      data.upper.vote.isUpTop = true;\n      topReplies.push(data.upper.vote);\n    } // \n\n\n    if (p === 1 && !me.next && me.sort === 0 && me.mode !== 2 && me.mode !== 3) {\n      if (me.isViewHot) {\n        topReplies = topReplies.concat(data.hots || []);\n      }\n\n      pos = data.hots && data.hots.length > 0 ? topReplies.length : -1;\n      me._pos = pos;\n    }\n\n    var arr;\n\n    if (data.replies instanceof Array) {\n      arr = topReplies.length > 0 ? me._uniq(topReplies.concat(data.replies)) : topReplies.concat(data.replies);\n    } else {\n      arr = topReplies.length > 0 ? me._uniq(topReplies) : topReplies;\n    }\n\n    if (this.noPage && this.pageSize) {\n      arr = arr.slice(0, this.pageSize);\n    }\n\n    me._renderReplyList(arr, pos, isLastPage);\n  }; // \n\n\n  Feedback.prototype._uniq = function (a) {\n    var seen = {};\n    return a.filter(function (item) {\n      return seen.hasOwnProperty(item.rpid) ? false : seen[item.rpid] = true;\n    });\n  };\n  /**\n   * \n   * @private\n   */\n\n\n  Feedback.prototype._renderInteractionManage = function () {\n    var me = this;\n    $.ajax({\n      url: api.interactionStatus,\n      type: 'GET',\n      data: {\n        type: this.pageType,\n        oid: this.oid\n      },\n      xhrFields: {\n        withCredentials: true\n      }\n    }).done(function (d) {\n      if (d && d.code === 0) {\n        me._rednderInteractionDom(d.data);\n      }\n    });\n  };\n\n  Feedback.prototype._rednderInteractionDom = function (data) {\n    var selection = data.up_reply_selection,\n        reply = data.up_reply,\n        dm = data.up_dm;\n    this.interactionStatus = {\n      selection: selection.status,\n      reply: reply.status,\n      dm: dm.status\n    };\n\n    if (!data.can_modify) {\n      return;\n    }\n\n    var html = ['<div class=\"operation ineraction-operation\">', '<div class=\"spot\"></div>', '<div class=\"opera-list\">', '<ul>', selection.can_modify ? '<li class=\"up_reply_selection\">' + (selection.status ? '' : '') + '</li>' : '', reply.can_modify ? '<li class=\"up_reply\">' + (reply.status ? '' : '') + '</li>' : '', dm.can_modify ? '<li class=\"up_dm\">' + (dm.status ? '' : '') + '</li>' : '', '</ul>', '</div>', '</div>'].join('');\n    this.$root.find('.header-interaction').html(html);\n  };\n  /**\n   * \n   * @param {Object} pageInfo - \n   * @private\n   */\n\n\n  Feedback.prototype._renderBottomPagination = function (pageInfo) {\n    if (this.noPage) {\n      var isLastPage = pageInfo.count <= this.pageSize;\n      var html = '';\n\n      if (isLastPage) {\n        html = '';\n      } else {\n        html = '<a class=\"more-link\" href=\"javascript:\"></a>';\n      }\n\n      this.$root.find('.bottom-page').addClass('center').html(html);\n      return;\n    }\n\n    var me = this;\n    var count = Math.ceil(pageInfo.count / pageInfo.size);\n\n    if (count > 1) {\n      this.$root.find('.bottom-page').paging({\n        pageCount: count,\n        current: pageInfo.num,\n        jump: utils.browser.version.mobile ? false : true,\n        smallSize: this.smallPager,\n        backFn: function backFn(p) {\n          me.$root.tri