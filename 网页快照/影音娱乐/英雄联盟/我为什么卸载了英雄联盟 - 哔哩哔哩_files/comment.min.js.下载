'].join('');\n    }\n\n    if (this.appMode === 'comic') return '';\n\n    if (fansMedal) {\n      // fansMedal.level 1-20  21 + \n      var backgroundColorStart = utils.hexToRgbA(fansMedal.medal_color.toString(16));\n      var backgroundColorEnd = utils.hexToRgbA(fansMedal.medal_color_end.toString(16));\n      var broderColor = utils.hexToRgbA(fansMedal.medal_color_border.toString(16));\n      var levelColor = utils.hexToRgbA(fansMedal.medal_color_level.toString(16));\n      var nameColor = utils.hexToRgbA(fansMedal.medal_color_name.toString(16));\n      var guardLevel = fansMedal.guard_level;\n\n      var _metrics2 = this._getTextMetrics(fansMedal.medal_name, {\n        fontWeight: 400,\n        fontSize: 10,\n        fontFamily: fontFamily\n      });\n\n      var medalNameWidth = Math.ceil(_metrics2.width);\n      return [\"<div class=\\\"medal\\\" style=\\\"padding-left:\".concat(guardLevel > 0 ? '8' : '0', \"px;\\\">\"), \"<img class=\\\"true-love-flag\\\" src=\\\"\".concat(utils.trimHttp(utils.webp(medalGuardLevel[guardLevel - 1], {})), \"\\\" style=\\\"display: \").concat(guardLevel > 0 ? 'block' : 'none', \";\\\">\"), \"<div class=\\\"true-love\\\" style=\\\"width: \".concat(medalNameWidth + 2, \"px; padding-left: \").concat(guardLevel > 0 ? '8' : '2', \"px; border-color:\").concat(broderColor, \"; color:\").concat(nameColor, \"; background-image: linear-gradient(90deg, \").concat(backgroundColorStart, \", \").concat(backgroundColorEnd, \")\\\">\"), \"<div class=\\\"tinyfont\\\">\".concat(fansMedal.medal_name, \"</div>\"), '</div>', \"<div class=\\\"medal-level\\\" style=\\\"border-color:\".concat(broderColor, \";color:\").concat(levelColor, \"\\\">\"), \"<div class=\\\"tinyfont\\\">\".concat(fansMedal.level, \"</div>\"), '</div>', '</div>'].join(''); // return '<span class=\"stick true-love\" style=\"border-color:' + broderColor + ';color:' + nameColor + '\">' \n      //   + fansMedal.medal_name \n      //   + '<span class=\"medal-level\" style=\"border-col