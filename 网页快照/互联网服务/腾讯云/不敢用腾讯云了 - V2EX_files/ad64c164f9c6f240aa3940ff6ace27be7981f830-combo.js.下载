function purgeStatusDraft(memberId){const contentId="status:by:"+memberId+":draft";lscache.remove(contentId);console.log("Purged status draft by "+memberId);}
function saveNoteDraft(memberId){const contentId="note:by:"+memberId+":draft";const draft=$("#note_content").val();lscache.set(contentId,draft,525600);console.log('Note draft by '+memberId+' is saved');}
